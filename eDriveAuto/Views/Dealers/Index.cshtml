										<div class="register-message">
											@(ViewData["Msg"] ?? "")
										</div>
										@Html.Partial("_DealerRegisteration", new _DealerRegisteration())
									</div>
									<div class="hint-box">
										<div class="htmlcontent">
											<div class="htmlcontent-body">
												@if(ViewData["DealerTextThree"] != null)
