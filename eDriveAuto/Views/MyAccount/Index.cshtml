                $(this).parents("#SecondBox").find(".NewsIn").show();
            });

            $("#SecondBox .Tab_01").click(function () {
                $(this).parent(".BoxTabs").removeClass("Tab_Second");
                $(this).parents("#SecondBox").find(".NewsIn").hide();
                $(this).parents("#SecondBox").find(".TestimonialsIn").show();
            });
        });

        $(document).ready(function () {
            $("#showBox_01").click(function () {
                $(this).parents("#newTabsBox").find(".tabStatus").removeClass("ajax__tab_active");
                $(this).addClass("ajax__tab_active");
                $(this).parents("#newTabsBox").find("#newTab_02").hide();
                $(this).parents("#newTabsBox").find("#newTab_01").show();
            });

            $("#showBox_02").click(function () {
                $(this).parents("#newTabsBox").find(".tabStatus").removeClass("ajax__tab_active");
                $(this).addClass("ajax__tab_active");
                $(this).parents("#newTabsBox").find("#newTab_01").hide();
                $(this).parents("#newTabsBox").find("#newTab_02").show();
            });
        });

    </script>
}
 
